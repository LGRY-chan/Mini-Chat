discord:
  enable-chat-redirection: false
  token: "YOUR_TOKEN_HERE"
  webhook-url: "WEBHOOK_URL_HERE"
  chat-redirection-channel: "CHANNEL_ID_HERE"
  bot-usable-channel: "CHANNEL_ID_HERE"
  activity: "오늘 날짜: %DAY%"

  message:

    boot:
      title: "프록시가 시작되었어요"
      show-server-count: true
      server-count: "활성화된 서버 수"
      show-start-time: true
      start-time: "시작 시각"

    join:
      simple: false
      title: "%PLAYER% 님이 접속했어요"
      description: "또 만났네요!"
      description-new: "첫 접속이네요. 반가워요!"
      description-old: "돌아온 것을 환영해요!"
      show-joined-server: true
      joined-server: "참가한 서버"
      show-start-time: true
      start-time: "참가한 시각"

    leave:
      simple: false
      title: "%PLAYER% 님이 종료했어요"
      description: "또 만나길 바랄게요."
      description-hard: "게임도 좋지만 조금 쉬는 게 어떨까요..."
      show-play-time: true
      play-time: "플레이한 시간"

    move:
      simple: false
      title: "%PLAYER% 님이 %SERVER_1%로 이동했어요"
      description: "또 만났네요!"
      show-previous-server: true
      previous-server: "이전 서버"




minecraft:
  enable-chat-redirection: true
  ignored-server: # in server which this list contains, all chat would be ignored, and no chat would be redirected to it.
    - limbo
    - banned-users

  server-name-replacement:
    s1: "야생 1"
    s2: "SMP 2"

  #message:
    #chat: "[%SERVER%] <%PLAYER%> %MESSAGE%"
    #join: "%PLAYER%이(가) [%SERVER%]에 참가했습니다"
    #leave: "%PLAYER%이(가) [%SERVER%]에서 떠났습니다"

others:
  enable-chat-logging: false
  server-owner: "LGRY"
  server-address: "LGRY.kro.kr"



